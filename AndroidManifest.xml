<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="demo.vicshady.coreshopping"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="19" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:name="demo.vicshady.coreshopping.app.CoreShopping"
        android:theme="@style/Theme.Etsy" >
        <activity
            android:name="demo.vicshady.coreshopping.ui.HomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="demo.vicshady.coreshopping.ui.SubCategoryActivity"/>
        <activity android:name="demo.vicshady.coreshopping.ui.ProductActivity"/>
        <activity android:name="demo.vicshady.coreshopping.ui.ProductDetailActivity"/>
        <activity android:name="demo.vicshady.coreshopping.ui.SearchProductActivity"/>
        <activity android:name="demo.vicshady.coreshopping.ui.ProductFeatureActivity"/>
    </application>

</manifest>
